[user]
    name = Maia
    email = maia.mcc@gmail.com
[core]
	editor = vim
[color]
    ui = auto
[alias]
    a = add -A
    d = diff
    dno = diff --name-only
    st = status
    co = checkout
    cp = cherry-pick
    b = branch
    cob = checkout -b
    cm = commit -m
    ca = commit --amend --no-edit
    cae = commit --amend # commit amend EDIT
    log = log --stat
    l = log
    lol = log --oneline -n 10
    graph = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %C(bold blue)<%an>%Creset %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
    pull = pull --rebase
    pullom = pull origin master
    pushom = push origin master
    rebm = rebase master
    rebi = rebase -i
    rebcont = rebase --continue
    rebabt = rebase --abort
    rest = reset head~
[grep]
    lineNumber = true
[push]
	default = simple
